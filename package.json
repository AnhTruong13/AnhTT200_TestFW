{
  "name": "anhtt200-test-fw",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:video": "npx playwright test --headed --project=chromium",
    "test:homepage": "npx playwright test tests/homepage.spec.ts",
    "test:integration": "npx playwright test tests/homepage-integration.spec.ts",
    "test:screenshots": "cross-env SCREENSHOTS=all npx playwright test",
    "test:minimal": "cross-env SCREENSHOTS=off npx playwright test",
    "test:screenshots:chromium": "cross-env SCREENSHOTS=all npx playwright test --project=chromium",
    "test:minimal:chromium": "cross-env SCREENSHOTS=off npx playwright test --project=chromium",
    "test:screenshots:line": "cross-env SCREENSHOTS=all npx playwright test --reporter=line",
    "test:minimal:line": "cross-env SCREENSHOTS=off npx playwright test --reporter=line",
    "test:video:all": "cross-env VIDEO_MODE=all SCREENSHOTS=all npx playwright test",
    "test:video:all:chromium": "cross-env VIDEO_MODE=all SCREENSHOTS=all npx playwright test --project=chromium",
    "test:evidence:full": "cross-env VIDEO_MODE=all SCREENSHOTS=all npx playwright test",
    "test:evidence:minimal": "cross-env VIDEO_MODE=off SCREENSHOTS=off npx playwright test",
    "report": "npx allure serve allure-results",
    "report:generate": "npx allure generate allure-results --output allure-report --clean",
    "report:open": "npx allure open allure-report",
    "clean:evidence": "npx rimraf Evidence/video Evidence/screenshots Evidence/traces allure-results test-results",
    "install:browsers": "npx playwright install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.3.0",
    "allure-commandline": "^2.34.1",
    "allure-js-commons": "^3.3.3",
    "allure-playwright": "^3.3.3",
    "cross-env": "^10.0.0",
    "rimraf": "^6.0.1"
  }
}